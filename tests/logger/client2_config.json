{
  "version": 1,
  "disable_existing_loggers": false,
  "loggers": {
    "HazelcastClient.MyClient2": {
      "level": "ERROR",
      "handlers": ["StdoutHandler"]
    }
  },

  "handlers": {
    "StdoutHandler": {
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "level": "ERROR"
    }
  }
}